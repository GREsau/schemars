{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Process",
  "type": "object",
  "required": [
    "command_line",
    "wall_time"
  ],
  "properties": {
    "command_line": {
      "type": "string"
    },
    "system_cpu_time": {
      "default": "0.000000000s",
      "allOf": [
        {
          "$ref": "#/definitions/Duration"
        }
      ]
    },
    "user_cpu_time": {
      "default": {
        "nanos": 0,
        "secs": 0
      },
      "allOf": [
        {
          "$ref": "#/definitions/Duration"
        }
      ]
    },
    "wall_time": {
      "$ref": "#/definitions/Duration"
    }
  },
  "definitions": {
    "Duration": {
      "type": "object",
      "required": [
        "nanos",
        "secs"
      ],
      "properties": {
        "nanos": {
          "type": "integer",
          "format": "int32"
        },
        "secs": {
          "type": "integer",
          "format": "int64"
        }
      }
    }
  }
}