{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "zero": {
      "type": "integer"
    },
    "one": {
      "type": "integer"
    },
    "minusOne": {
      "type": "integer"
    },
    "zeroPointZero": {
      "type": "number"
    },
    "bool": {
      "type": "boolean"
    },
    "null": true,
    "object": {
      "type": "object",
      "properties": {
        "array": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "examples": [
    {
      "zero": 0,
      "one": 1,
      "minusOne": -1,
      "zeroPointZero": 0.0,
      "bool": true,
      "null": null,
      "object": {
        "array": [
          "foo",
          "bar"
        ]
      }
    }
  ]
}