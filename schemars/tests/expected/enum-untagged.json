{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Untagged",
  "anyOf": [
    {
      "type": "null"
    },
    {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    {
      "type": "object",
      "properties": {
        "bar": {
          "type": "boolean"
        },
        "foo": {
          "type": "integer",
          "format": "int32"
        }
      },
      "required": [
        "bar",
        "foo"
      ]
    },
    {
      "type": "array",
      "items": [
        {
          "type": "integer",
          "format": "int32"
        },
        {
          "type": "boolean"
        }
      ],
      "maxItems": 2,
      "minItems": 2
    }
  ]
}