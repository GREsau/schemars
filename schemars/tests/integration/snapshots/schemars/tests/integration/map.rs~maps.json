{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Maps",
  "type": "object",
  "properties": {
    "s_map": {
      "type": "object",
      "additionalProperties": {
        "type": "boolean"
      }
    },
    "i_map": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        "^-?\\d+$": {
          "type": "boolean"
        }
      }
    },
    "u_map": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        "^\\d+$": {
          "type": "boolean"
        }
      }
    },
    "pattern_map": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        "^[0-9a-f]*$": {
          "type": "boolean"
        }
      }
    },
    "enum_map": {
      "type": "object",
      "properties": {
        "Unit1": {
          "type": "boolean"
        },
        "Unit2": {
          "type": "boolean"
        }
      },
      "additionalProperties": false,
      "patternProperties": {
        "^-?\\d+$": {
          "type": "boolean"
        }
      }
    }
  },
  "required": [
    "s_map",
    "i_map",
    "u_map",
    "pattern_map",
    "enum_map"
  ],
  "$defs": {
    "IndirectU32": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0
    }
  }
}